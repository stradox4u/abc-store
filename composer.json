{
  "name": "stradox/abc-store",
  "description": "Coding Assignment from abc.pl",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Stradox\\AbcStore\\": "src/"
    },
    "classmap": [
      "config",
      "src/models"
    ]
  },
  "authors": [
    {
      "name": "Umar Adejoh",
      "email": "umar@arcodeh.pro"
    }
  ],
  "scripts": {
    "database:drop": "vendor/bin/doctrine orm:schema-tool:drop --force",
